{
	"name": "Backend",
	"dockerComposeFile": [
		"../docker-compose.yml",
		"../docker-compose.devcontainer.yml"
	],
	"service": "backend",
	"workspaceFolder": "/usr/src/app",
	"settings": {
		"terminal.integrated.shell.linux": "/bin/zsh"
	},
	"extensions": [
		"ms-python.python"
	],
	"shutdownAction": "none",
	"forwardPorts": [
		8000,
		8001
	]
}